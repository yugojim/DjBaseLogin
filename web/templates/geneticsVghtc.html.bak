{% load static %}
<!doctype html>
<html lang="en">
  <head>
    <title>Precision Medicine Genetic Information</title>

    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="{% static '' %}css/bootstrap.min.css">

  </head>
  
  {% if request.user.is_authenticated %}
  <body>
    <div class="d-flex flex-column flex-md-row align-items-center p-3 px-md-4 mb-3 bg-white border-bottom shadow-sm">
	  <h5 class="my-0 mr-md-auto font-weight-normal">NYMU DC&copy; 2017–2021</h5>
	  <nav class="my-2 my-md-0 mr-md-3">
		<a class="p-2 text-dark" href="/admin">Admin</a>
		<a class="p-2 text-dark" href="#">Enterprise</a>
		<a class="p-2 text-dark" href="#">Support</a>
	  </nav>
	  <a class="btn btn-outline-primary" href="{% url 'web:logout' %}">Logout</a>
	</div>
		<div class="container">
			<form method="post">{% csrf_token %}
			  <div class="form-row" style='text-align: center;'>
				<div class="form-group col-md-3">
				  <h2>GUID or Medical record number</h2>
				</div>
				<div class="form-group col-md-6">
					<input name="PatientID" placeholder="" value="060PM08752258F07-068" class="form-control">
				</div>
				<div class="form-group col-md-3">
					<center>
						<button type="submit" class="btn-lg btn-light">Search</button>
					</center>
				</div>
			  </div>
			</form>
			{% if PatientID %}
			<br>
			<br>
			<div class="row">
			  <div class="col-sm-2"><center><img src="{% static '' %}210204-NYCU.jpg" alt="vghtc_log"></center></div>
			  <div class="col-sm-10"><center><h1>Precision Medicine Genetic Information</h1></center></div>
			</div>
			<table class="table table-bordered">
			  <tbody>
				<tr>
				  <td>Collection date</td>
				  <td>***</</td>
				  <td>Receipt date</td>
				  <td>***</</td>
				  <td>Report date</td>
				  <td>***</</td>
				</tr>
				<tr>
				  <td>Report No</td>
				  <td>Medical record number</td>
				  <td>Name</td>
				  <td>ID number</td>
				  <td>Birthday</td>
				  <td>Specimen</td>
				</tr>
				<tr>
				  <td>***</td>
				  <td>{{ PatientID }}</td>
				  <td>***</td>
				  <td>***</td>
				  <td>***</td>
				  <td>blood</td>
				</tr>
				<tr>
				  <td>Inspection Entity</td>
				  <td colspan="5">***</</td>
				</tr>
			  </tbody>
			</table>
			<table class="table table-bordered">
			  <thead>
				<tr>
				  <th scope="col">Index</th>
				  <th scope="col">Item</th>
				  <th scope="col">Gene</th>
				  <th scope="col">Risk</th>
				  <th scope="col">Health education leaflet</th>
				</tr>
			  </thead>
			  <tbody>
				<tr>
				  <td>1</td>
				  <td><a href="/risk?risk=Alc_risk" title="" onclick="window.open(this.href, 'mywin','left=20,top=20,width=500,height=500,toolbar=1,resizable=0'); return false;">alcohol metabolism</a>
				  </td>
				  <td><a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:404" 
						
						{% if ALDH2list %}
							title="
								{% for ALDH2rs in ALDH2list %}{{ ALDH2rs }}&#10;{% endfor %}"
						{% else %}
							title=""
						{% endif %} 
						target="_blank">ALDH2
					</a>
				  </td>
				  <td><h5>
					<span
						{% if ALDH2list %}
							{% if ALDH2list.0 == "G/G at SNP rs671" %}class="badge badge-success">normal
							{% else %}class="badge badge-warning">riskless
							{% endif %}
						{% endif %}
					</span>
				  </h5></td>
				  <td><a href="https://www.cmuh.cmu.edu.tw/HealthEdus/Detail?no=5405" target="_blank">leaflet
					</a></td>
				</tr>
				<tr>
				  <td>2</td>
				  <td><a href="/risk?risk=Obes_risk" title="" onclick="window.open(this.href, 'mywin','left=20,top=20,width=500,height=500,toolbar=1,resizable=0'); return false;">obesity</a>
				  </td>
				  <td>
					<a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:24678" 
						{% if Obes_list %}title="{% for Obesrs in Obes_list %}{{ Obesrs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">FTO
					</a>
				  </td>
				  <td><h5>
						<span
							{% if Obes_risk == "較高風險" %}
							class="badge badge-danger">risk
							</span></h5></td>
							<td><a href="https://www.cmuh.cmu.edu.tw/HealthEdus/Detail?no=4789" target="_blank">leaflet
							</a></td>
							{% elif Obes_risk == "中度風險" %}
							class="badge badge-warning">riskless
							</span></h5></td>
							<td><a href="https://www.cmuh.cmu.edu.tw/HealthEdus/Detail?no=4789" target="_blank">leaflet
							</a></td>
							{% elif Obes_risk == "未檢出異常" %}
							class="badge badge-success">normal
							</span></h5></td>
							{% else %}
							class="badge badge-secondary">none
							</span></h5></td>
							{% endif %}
				</tr>
				<tr>
				  <td>3</td>
				  <td><a href="/risk?risk=ABCG2_risk" title="" onclick="window.open(this.href, 'mywin','left=20,top=20,width=500,height=500,toolbar=1,resizable=0'); return false;">Uric acid metabolism</a></td>
				  <td>
					<a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:74" 
						{% if ABCG2list %}title="{% for ABCG2rs in ABCG2list %}{{ ABCG2rs }}&#10;{% endfor %}"{% else %}title=""{% endif %} 
						target="_blank">ABCG2
					</a>
				  </td>
				  <td><h5>				  
					<span
						{% if ABCG2list %}
							{% if ABCG2list.0 == "G/G at SNP rs2231142" %}
								class="badge badge-success">normal
								</span></h5></td>
							{% else %}
								class="badge badge-warning">riskless
								</span></h5></td>
								<td><a href="https://www.cmuh.cmu.edu.tw/HealthEdus/Detail?no=4864" target="_blank">leaflet</a></td>
							{% endif %}
						{% endif %}
					

				</tr>
				<tr>
				  <td>4</td>
				  <td><a href="/risk?risk=HBB_risk" title="" onclick="window.open(this.href, 'mywin','left=20,top=20,width=500,height=500,toolbar=1,resizable=0'); return false;">beta-thalassemia</a></td>
				  <td>
					<a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:4827" 
						{% if HBBlist %}title="{% for HBBrs in HBBlist %}{{ HBBrs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">HBB
					</a>
				  </td>
				  <td><h5>
						<span
							{% if HBB_risk == "較高風險" %}
							class="badge badge-danger">risk
							</span></h5></td>
							<td><a href="https://www.cmuh.cmu.edu.tw/HealthEdus/Detail?no=5081" target="_blank">leaflet
							</a></td>
							{% elif HBB_risk == "中度風險" %}
							class="badge badge-warning">riskless
							</span></h5></td>
							<td><a href="https://www.cmuh.cmu.edu.tw/HealthEdus/Detail?no=5081" target="_blank">leaflet
							</a></td>
							{% elif HBB_risk == "未檢出異常" %}
							class="badge badge-success">normal
							</span></h5></td>
							{% else %}
							class="badge badge-secondary">none
							</span></h5></td>
							{% endif %}
				</tr>
				<tr>
				  <td>5</td>
				  <td>
					<a href="/risk?risk=FH_risk" title="" onclick="window.open(this.href, 'mywin','left=20,top=20,width=500,height=500,toolbar=1,resizable=0'); return false;">familial hypercholesterolemia</a>					
				  </td>
				  <td>
					<a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:6547" 
						{% if LDLRlist %}title="{% for LDLRrs in LDLRlist %}{{ LDLRrs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">LDLR
					</a>
				  、
				  <a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:20001" 
						{% if PCSK9list %}title="{% for PCSK9rs in PCSK9list %}{{ PCSK9rs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">PCSK9
					</a>				  
				  、
				  <a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:603" 
						{% if PCSK9list %}title="{% for APOBrs in APOBlist %}{{ APOBrs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">APOB
					</a>
				  </td>
				  <td><h5>
						<span
							{% if FH_risk == "較高風險" %}
							class="badge badge-danger">risk
							</span></h5></td>
							<td><a href="https://www.cmuh.cmu.edu.tw/HealthEdus/Detail?no=4859" target="_blank">leaflet
							</a></td>
							{% elif FH_risk == "中度風險" %}
							class="badge badge-warning">riskless
							</span></h5></td>
							<td><a href="https://www.cmuh.cmu.edu.tw/HealthEdus/Detail?no=4859" target="_blank">leaflet
							</a></td>
							{% elif FH_risk == "未檢出異常" %}
							class="badge badge-success">normal
							</span></h5></td>
							{% else %}
							class="badge badge-secondary">none
							</span></h5></td>
							{% endif %}
				</tr>
				<tr>
				  <td>6</td>
					  <td>
						  <a href="/risk?risk=CADASIL_risk" title="" onclick="window.open(this.href, 'mywin','left=20,top=20,width=500,height=500,toolbar=1,resizable=0'); return false;">hereditary cerebral arteriopathy
						  </a>
					  </td>
				  <td>
					<a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:4827" 
						{% if NOTCH3list %}title="{% for NOTCH3rs in NOTCH3list %}{{ NOTCH3rs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">
						NOTCH3
					</a>
				  </td>
				  <td><h5>
					<span
						{% if NOTCH3list %}
							{% if NOTCH3list.0 == "G/G at SNP rs201118034" %}
							class="badge badge-success">normal
							</span></h5></td>
							{% else %}
							class="badge badge-warning">riskless
							</span></h5></td>
							<td><a href="https://www.cmuh.cmu.edu.tw/HealthEdus/Detail?no=4847" target="_blank">leaflet
							</a></td>
							{% endif %}
						{% endif %}
				</tr>
				<tr>
				  <td>7</td>
					  <td>
						 <a href="/risk?risk=MODY_risk" title="" onclick="window.open(this.href, 'mywin','left=20,top=20,width=500,height=500,toolbar=1,resizable=0'); return false;">Adult-onset diabetes in young adults				  </a>						
					  </td>
				  <td>
					<a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:59" 
						{% if ABCC8list %}title="{% for ABCC8Rrs in ABCC8list %}{{ ABCC8Rrs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">ABCC8
					</a>、
					<a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:4195" 
						{% if GCKlist %}title="{% for GCKRrs in GCKlist %}{{ GCKRrs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">GCK
					</a>、
					<a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:5024" 
						{% if HNF1Alist %}title="{% for HNF1ARrs in HNF1Alist %}{{ HNF1ARrs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">HNF1A
					</a>、
					<a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:11621" 
						{% if HNF4Alist %}title="{% for HNF4ARrs in HNF4Alist %}{{ HNF4ARrs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">HNF4A
					</a>、
					<a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:6257" 
						{% if KCNJ11list %}title="{% for KCNJ11Rrs in KCNJ11list %}{{ KCNJ11Rrs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">KCNJ11
					</a>
					</td>
				  <td><h5>
						<span
							{% if MODY_risk == "較高風險" %}
							class="badge badge-danger">risk
							</span></h5></td>
							<td><a href="https://www.cmuh.cmu.edu.tw/HealthEdus/Detail?no=4802" target="_blank">leaflet
							</a></td>
							{% elif MODY_risk == "中度風險" %}
							class="badge badge-warning">riskless
							</span></h5></td>
							<td><a href="https://www.cmuh.cmu.edu.tw/HealthEdus/Detail?no=4802" target="_blank">leaflet
							</a></td>
							{% elif MODY_risk == "未檢出異常" %}
							class="badge badge-success">normal
							</span></h5></td>
							{% else %}
							class="badge badge-secondary">none
							</span></h5></td>
							{% endif %}
				</tr>
				<tr>
				  <td>8</td>
					<td>
						 <a href="/risk?risk=CYP2C19_risk" title="" onclick="window.open(this.href, 'mywin','left=20,top=20,width=500,height=500,toolbar=1,resizable=0'); return false;">Adult-onset diabetes in young adults				  </a>
					</td>
				  <td>
					<a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:4827" 
						{% if CYP2C19list %}title="{% for CYP2C19rs in CYP2C19list %}{{ CYP2C19rs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">
						CYP2C19
					</a>			  
				  </td>
				  <td><h5>
					<span
						{% if CYP2C19_risk == "較高風險(慢速代謝者)" %}class="badge badge-danger">risk
						{% elif CYP2C19_risk == "中度風險(中速代謝者)" %}class="badge badge-warning">riskless
						{% elif CYP2C19_risk == "未檢出異常" %}class="badge badge-success">normal
						{% else %}class="badge badge-secondary">none
						{% endif %}
					</span>	
				  </h5></td>

				</tr>
				<tr>
				  <td>9</td>
					<td>
					  <a href="/risk?risk=TPMT_risk" title="" onclick="window.open(this.href, 'mywin','left=20,top=20,width=500,height=500,toolbar=1,resizable=0'); return false;">drug metabolizing enzyme TPMT
					  </a>						
					</td>
					<td>
						<a href="https://www.genenames.org/data/gene-symbol-report/#!/hgnc_id/HGNC:12014" 
							{% if TPMTlist %}title="{% for TPMTrs in TPMTlist %}{{ TPMTrs }}&#10;{% endfor %}"{% else %}title=""{% endif %} target="_blank">TPMT
						</a>						
					</td>
				  <td><h5>
					<span
						{% if TPMT_risk == "較高風險" %}class="badge badge-danger">risk
						{% elif TPMT_risk == "中度風險" %}class="badge badge-warning">riskless
						{% elif TPMT_risk == "未檢出異常" %}class="badge badge-success">normal
						{% else %}class="badge badge-secondary">none
						{% endif %}
					</span>	
				  </h5></td>
				</tr>
				<tr>
				  <td>10</td>
				  <td>Uric acid-lowering drug Allopurinol allergic gene</td>
					<td>
						HLA–B*58:01
					</td>
				  <td><h5><span class="badge badge-secondary">none</span></h5></td>
				</tr>
				<tr class="table-active"><td colspan="5"><center>Summarize</center></td></tr>
				<tr>					
					<td><a href="https://www.cmuh.cmu.edu.tw/HealthEdus?type=1" target="_blank">Health education leaflet
				    </a></td>
					<td colspan="4">
						{% if final_comment %}{{ final_comment }}{% else %}normal{% endif %}
					</td>
				</tr>
			  </tbody>
			</table>
			{% endif %}
		</div>
	{% else %}
	<link rel="stylesheet" type="text/css" href="{% static '' %}admin/css/base.css">
	<link rel="stylesheet" type="text/css" href="{% static '' %}admin/css/nav_sidebar.css">
	<script src="{% static '' %}admin/js/nav_sidebar.js" defer></script>
	<link rel="stylesheet" type="text/css" href="{% static '' %}admin/css/login.css">
	<meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1.0, maximum-scale=1.0">
	<link rel="stylesheet" type="text/css" href="{% static '' %}admin/css/responsive.css">
	<meta name="robots" content="NONE,NOARCHIVE">
		<body class=" login" data-admin-utc-offset="28800">
		<!-- Container -->
			<div id="container">
				<!-- Header -->
				<div id="header">
					<div id="branding">
						<h1 id="site-name">Login</h1>
					</div>
				</div>
				<!-- END Header -->
				<div class="main" id="main">
				  <div class="content">
					<!-- Content -->
					<div id="content" class="colM">
						<div id="content-main">
							<form method="post" action="{% url 'web:login' %}">
							{% csrf_token %}
							  <div class="form-row">
								<label for="id_username" class="required">Username:</label> <input type="text" name="username" autofocus autocapitalize="none" autocomplete="username" maxlength="150" required id="id_username">
							  </div>
							  <div class="form-row">
								<label for="id_password" class="required">Password:</label> <input type="password" name="password" autocomplete="current-password" required id="id_password">
							  </div>
							  <div class="submit-row">
								<input type="submit" value="Log in">
							  </div>
							</form>
						</div>
					  <br class="clear">
					</div>
					<!-- END Content -->
		<center>
						<div id="footer">
							<p class="mt-5 mb-3 text-muted">
								<a href="{% url 'web:password_reset' %}">Forget Password?</a>    NYMU DC&copy; 2017–2021
							</p>
						</div>

				  </div>
				</div>
			</div>
		</center>
		<!-- END Container -->
    {% endif %}
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="{% static '' %}js/jquery-3.3.1.js"></script>
    <script src="{% static '' %}js/popper.min.js"></script>
    <script src="{% static '' %}js/bootstrap.min.js"></script>
  </body>
</html>